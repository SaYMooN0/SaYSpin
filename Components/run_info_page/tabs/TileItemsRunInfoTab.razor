@inject AppController gameController

<div class="tile-items-container">
    <div class="tile-items-grid ">

        @foreach (var tileItem in gameController.Game.AllTileItemsCollection)
        {
            <div>
                <InventoryItemWithRarityComponent itemToDisplay="@tileItem" />
            </div>
        }

    </div>
</div>
